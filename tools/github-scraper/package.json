{
  "name": "@aetherlight/github-scraper",
  "version": "1.0.0",
  "description": "Node 1 Deployment Sprint 2: GitHub repository scraper with quality filters",
  "main": "dist/scraper-orchestrator.js",
  "types": "dist/scraper-orchestrator.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/scraper-orchestrator.ts",
    "scrape": "npm run build && node dist/scraper-orchestrator.js",
    "scrape:marketing": "npm run build && DOMAIN=Marketing node dist/scraper-orchestrator.js",
    "scrape:legal": "npm run build && DOMAIN=Legal node dist/scraper-orchestrator.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "github",
    "scraper",
    "pattern-extraction",
    "aetherlight",
    "node1-deployment"
  ],
  "author": "Ã†therLight Team",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@octokit/plugin-throttling": "^8.1.3",
    "@octokit/plugin-retry": "^6.0.1",
    "@supabase/supabase-js": "^2.39.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
